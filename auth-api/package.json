{
  "name": "books_api",
  "license": "SEE LICENSE IN LICENSE",
  "type": "module",
  "scripts": {
    "start": "tsx ./bin/run_server.ts",
    "start:test": "dotenv -e .env.test -- npm run start",
    "watch": "tsx watch ./bin/run_server.ts",
    "db:migrate": "prisma migrate dev",
    "db:migrate:test": "dotenv -e .env.test -- prisma migrate dev",
    "db:seed": "prisma db seed",
    "db:seed:test": "dotenv -e .env.test -- prisma db seed",
    "db:reset": "prisma migrate reset",
    "db:reset:test": "dotenv -e .env.test -- prisma migrate reset --force",
    "test": "npm run db:reset:test && dotenv -e .env.test -- jest",
    "test:get_books": "npm run test -- get_books.ts",
    "test:get_book_by_id": "npm run test -- get_book_by_id.ts",
    "test:search_book_title": "npm run test -- search_book_title.ts",
    "test:create_book": "npm run test -- create_book.ts",
    "test:update_book_title": "npm run test -- update_book_title.ts",
    "test:delete_book_by_id": "npm run test -- delete_book_by_id.ts",
    "lint:check": "eslint",
    "lint:fix": "eslint --fix",
    "format:check": "prettier --check '**/*.{js,ts}'",
    "format:fix": "prettier --write '**/*.{js,ts}'",
    "typecheck": "npm run typecheck"
  },
  "dependencies": {
    "@prisma/client": "^6.2.1",
    "@types/cookie-parser": "^1.4.10",
    "@types/jsonwebtoken": "^9.0.10",
    "bcrypt": "^6.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^9.0.2",
    "prisma": "^6.2.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "cross-fetch": "^4.1.0",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "jest": "^29.7.0",
    "jest-dev-server": "^11.0.0",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.21.0"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "overrides": {
    "glob": "10.3.16"
  }
}
